<h1 id="sampleData" hidden="true">Sample Data</h1>
<table id="notesDataTable" class="display compact">
  <thead>
  <tr>
    <th>Loan ID</th>
    <th>Note ID</th>
    <th>Order ID</th>
    <th>Interest Rate</th>
    <th>Term</th>
    <th>Status</th>
    <th>Grade</th>
    <th>Amount</th>
    <th>Note Amount</th>
    <th>Payments Received</th>
    <th>Issue Date</th>
    <th>Order Date</th>
  </tr>
  </thead>
  <tbody></tbody>
</table>

<div id="statsWrapper" class="col-lg-12">
  <div id="interestRateWrapper" class="list-inline">
    <h3>Interest Rate</h3>
    <strong>Average</strong>
    <span id="averageInterestRate"></span>
    <strong>Maximum</strong>
    <span id="maximumInterestRate"></span>
    <strong>Minimum</strong>
    <span id="minimumInterestRate"></span>
  </div>
  <div id="paymentsWrapper" class="list-inline">
    <h3>Payments Received</h3>
    <strong>Average</strong>
    <span id="averagePayments"></span>
    <strong>Maximum</strong>
    <span id="maximumPayments"></span>
    <strong>Minimum</strong>
    <span id="minimumPayments"></span>
    <strong>Total</strong>
    <span id="totalPayments"></span>
  </div>
  <div id="accrualWrapper" class="list-inline">
    <h3>Accrued Interest</h3>
    <strong>Average</strong>
    <span id="averageAccrual"></span>
    <strong>Maximum</strong>
    <span id="maximumAccrual"></span>
    <strong>Minimum</strong>
    <span id="minimumAccrual"></span>
    <strong>Total</strong>
    <span id="totalAccrual"></span>
  </div>
  <div id="daysToIssueWrapper" class="list-inline">
    <h3>Days To Issue From Order Date</h3>
    <strong>Average</strong>
    <span id="averageDaysToIssue"></span>
    <strong>Maximum</strong>
    <span id="maximumDaysToIssue"></span>
    <strong>Minimum</strong>
    <span id="minimumDaysToIssue"></span>
    <strong>Total</strong>
    <span id="totalDaysToIssue"></span>
  </div>
</div>

<div id="notesWrapper">
  <div id="interestRateChart" style="width: 100%; height: 400px; background-color: #FFFFFF;"></div>
  <div id="gradeChart" style="width: 100%; height: 400px; background-color: #FFFFFF;"></div>
  <div id="loanAmountChart" style="width: 100%; height: 400px; background-color: #FFFFFF;"></div>
  <div id="paymentsReceivedChart" style="width: 100%; height: 400px; background-color: #FFFFFF;"></div>
  <div id="defaultsByGradeChart" style="width: 100%; height: 400px; background-color: #FFFFFF;"></div>
  <div id="defaultedDollarsByGradeChart" style="width: 100%; height: 400px; background-color: #FFFFFF;"></div>
  <div id="nextPaymentDateChart" style="width: 100%; height: 400px; background-color: #FFFFFF;"></div>
  <div id="issueDateChart" style="width: 100%; height: 400px; background-color: #FFFFFF;"></div>
  <div id="creditTrendChart" style="width: 100%; height: 400px; background-color: #FFFFFF;"></div>
</div>